{
  "name": "command-center",
  "version": "0.0.1",
  "description": "Personal command center site",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start": "node target"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "web/**"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AGrzes/command-center.git"
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AGrzes/command-center/issues"
  },
  "homepage": "https://github.com/AGrzes/command-center#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "connect-history-api-fallback": "^1.5.0",
    "debug": "^4.1.1",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "lodash": "^4.17.10",
    "marked": "^0.5.0",
    "moment": "^2.22.2",
    "ouch-rx": "^2.0.3",
    "pouchdb-http": "^6.0.2",
    "pouchdb-mapreduce": "^6.4.3",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/connect-history-api-fallback": "^1.3.1",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "@types/lodash": "^4.14.116",
    "@types/marked": "^0.4.1",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.9.2",
    "@types/pouchdb-http": "^6.1.2",
    "@types/pouchdb-mapreduce": "^6.1.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.21.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
